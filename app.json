{
  "expo": {
    "name": "TipsyTraveller",
    "slug": "TipsyTraveller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/ios-light.png",
      "bundleIdentifier": "com.nielspet.TipsyTraveller",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ios-light.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nielspet.TipsyTraveller"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "/assets/fonts/dogicapixel.otf",
            "/assets/fonts/dogicapixelbold.otf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fc6c433d-6c8c-4f79-b758-1d6b47ed5ee9"
      }
    }
  }
}
